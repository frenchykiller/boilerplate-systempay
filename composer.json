{
    "name": "frenchykiller/laravel-systempay",
    "description": "Laravel component to generate a payment form for Banque Populaire Systempay",
    "keywords": [
        "laravel",
        "systempay"
    ],
    "require": {
        "php": ">=7.4",
        "guzzlehttp/guzzle": "^7.0.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0|^9.0",
        "orchestra/testbench": "^4.0|^5.0|^6.0",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Christopher Walker",
            "email": "christophersvt3g@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Frenchykiller\\LaravelSystempay\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Frenchykiller\\LaravelSystempay\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Frenchykiller\\LaravelSystempay\\SystempayServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev"
}
